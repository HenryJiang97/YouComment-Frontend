    postComment(content, rating, videoId) {\n        const that = this;\n        Axios.post(\n            commentApiPrefix, \n            {\n              id: 'user1', //need to change later\n              rating: rating,\n              content: content,\n              videoId: videoId,\n              posterId:uuidv4(),\n            }\n          ).then(function() {\n              alert('successfully posted your comment');\n              that.setState({\n                showComments : '1',\n              })\n          })\n          .catch(error => console.log(error))\n          .then(function(){\n              that.setState({\n                content:'',\n                rating:'',\n              })\n              document.getElementById(\"content\").value = \"\";\n              document.getElementById(\"rating\").value = \"\";\n              that.updateCommentList();\n          })\n\n    }\n    onSubmit() {\n        const that = this;\n        const content = this.state.content;\n        const rating = this.state.rating;\n        const videoId = this.state.video.videoId;\n        console.log(content+ '........' + rating + '.......'+videoId);\n        that.postComment(content, rating, videoId);\n      }\n    \n\n    render () {\n        const video =this.state.video;\n        const commentList = this.state.commentList;\n        console.log(video);\n        console.log(commentList);\n\n\n        return (\n            <div>\n                <Table size = \"sm\">\n                    <thead>\n                        <tr>\n                            <th>Info</th>\n                            <th>Value</th>\n                        </tr>    \n                    </thead>\n                    <tbody>\n                        <tr>\n                            <td>Video Title</td>\n                            <td>{video.title}</td>\n                        </tr>\n                        <tr>\n                            <td>Description</td>\n                            <td>{video.description}</td>\n                        </tr>\n                        <tr>\n                            <td>Video Id</td>\n                            <td>{video.videoId}</td>\n                        </tr>\n                        <tr>\n                            <td>Publish Time</td>\n                            <td>{video.publishTime}</td>\n                        </tr>\n                        <tr>\n                            <td>Channel Title</td>\n                            <td>{video.channelTitle}</td>\n                        </tr>\n                        <tr>\n                            <td>Channel Id</td>\n                            <td>{video.channelId}</td>\n                        </tr>\n                        \n                    </tbody>\n                </Table>\n\n                <div>\n                    <div>\n                        <Button onClick={this.onView}>View Comments</Button>\n                    </div>\n                    {this.state.showComments === '1' ? <CommentList videoId={this.state.video.videoId} commentList={this.state.commentList} />: null}\n                </div>\n                \n\n\n                {/* Leave comments */}\n                <div>\n                    {this.state.user == undefined\n                    ?\n                        (\n                            <h4>You're not signed in</h4>\n                        )\n                    :\n                        (\n                            <div>\n                                <h5>New Comment:</h5>\n                                <div>Comment: \n                                    <input\n                                            id=\"content\"\n                                            onChange={this.onContentChange}></input>\n                                </div>\n                                <div>Rating(1~5):\n                                    <input\n                                            id=\"rating\"\n                                            onChange={this.onRatingChange}></input>\n                                </div>\n                                <div>\n                                    <button onClick={this.onSubmit}>Submit</button>\n                                </div>\n                            </div>\n                        )\n                    }\n                </div>\n                \n\n                <Link to={{\n                    pathname: \"/result\",\n                    query: {\n                        videosList: this.props.location.query.videosList,\n                        searchWord: \"\",\n                    }\n                }}>\n                    <Button>Back</Button>\n                </Link>\n            </div>\n        );\n    }\n}\n\n\nexport default Detail;\n\n\n\n\n               ","/Users/henryjiang/Documents/My documents/CS 5610/Project/Frontend/YouComment-Frontend/src/Config.js",[],"/Users/henryjiang/Documents/My documents/CS 5610/Project/Frontend/YouComment-Frontend/src/Videos/CommentList.js",[],"/Users/henryjiang/Documents/My documents/CS 5610/Project/Frontend/YouComment-Frontend/src/Videos/Comment.js",[],"/Users/henryjiang/Documents/My documents/CS 5610/Project/Frontend/YouComment-Frontend/src/Videos/Result.js",[],"/Users/henryjiang/Documents/My documents/CS 5610/Project/Frontend/YouComment-Frontend/src/User/Edit.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":9,"column":8,"nodeType":"80","messageId":"81","endLine":9,"endColumn":17},{"ruleId":"78","severity":1,"message":"82","line":4,"column":10,"nodeType":"80","messageId":"81","endLine":4,"endColumn":20},{"ruleId":"83","severity":1,"message":"84","line":164,"column":38,"nodeType":"85","messageId":"86","endLine":164,"endColumn":40},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'VideoList' is defined but never used.","Identifier","unusedVar","'useHistory' is defined but